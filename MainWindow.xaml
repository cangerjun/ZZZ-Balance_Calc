    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" Width="900" Height="1080" Background="#FF23272E" HorizontalAlignment="Left" VerticalAlignment="Top">

        <Grid Height="1000" Width="900" VerticalAlignment="Top">
            <Border Background="#FF1E2227" Height="30" VerticalAlignment="Top" MouseDown="拖动窗口">
                <Grid>
                    <!-- 标题栏内容 -->
                    <TextBlock Text="绝区零属性权重计算器 V2.0.1" Foreground="White" VerticalAlignment="Center" FontSize="16" HorizontalAlignment="Left" Margin="10,0,0,0"/>

                    <!-- 右对齐按钮 -->
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Center">
                        <Button Content="一" Width="40" Click="最小化_Click" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Height="30"/>
                        <Button Content="X" Width="40" Click="关闭_Click" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Height="30"/>
                    </StackPanel>
                </Grid>
            </Border>

            <RichTextBox x:Name="cmd输出" Margin="10,95,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="880" Height="135" Background="Black" SelectionBrush="White" BorderBrush="White" Foreground="White" SelectionTextBrush="{x:Null}" Grid.ColumnSpan="3" ScrollViewer.VerticalScrollBarVisibility="Auto" IsReadOnly="True" IsReadOnlyCaretVisible="True" FontSize="16" >
                <FlowDocument>
                    <Paragraph>
                        <Run Text="运行状态:"/>
                    </Paragraph>
                </FlowDocument>
